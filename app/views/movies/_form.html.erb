<%= form_for @movie do |f| %>

	<%= f.label 'Title' %>
	<%= f.text_field :title %></br>

	<%= f.label 'Gif' %>
	<%= f.text_field :gif %></br>

	<%= f.label 'Trailer' %>
	<%= f.text_field :trailer %></br>

	<%= f.label 'Description' %>
	<%= f.text_field :description %></br>

	<%= f.submit 'Submit New Movie' %>

<% end %>